<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>

<GridPane styleClass="status-bar" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" minHeight="25" prefHeight="25" maxHeight="25">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
  </columnConstraints>
  <padding>
    <Insets top="3" right="5" bottom="3" left="5" />
  </padding>
  <Label fx:id="saveLocationStatus" GridPane.columnIndex="0" />
</GridPane>
